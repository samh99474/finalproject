<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="69dp"
        android:layout_height="44dp"
        android:text="電郵"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/ed_4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ed_2"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/ed_4"
        android:layout_width="291dp"
        android:layout_height="59dp"
        android:layout_marginBottom="216dp"
        android:ems="10"
        android:hint="輸入電郵"
        android:inputType="textPersonName"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="69dp"
        android:layout_height="44dp"
        android:text="年齡"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/ed_3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ed_2"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/ed_3"
        android:layout_width="291dp"
        android:layout_height="59dp"
        android:layout_marginBottom="320dp"
        android:ems="10"
        android:hint="輸入年齡"
        android:inputType="textPersonName"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <EditText
        android:id="@+id/ed_2"
        android:layout_width="291dp"
        android:layout_height="59dp"
        android:layout_marginBottom="436dp"
        android:ems="10"
        android:hint="輸入車牌"
        android:inputType="textPersonName"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="69dp"
        android:layout_height="44dp"
        android:text="車牌"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/ed_2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ed_1"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="69dp"
        android:layout_height="44dp"
        android:layout_marginTop="64dp"
        android:text="姓名"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/ed_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_personalinfo"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:textAlignment="center"
        android:textSize="1sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="個人檔案"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/ed_1"
        android:layout_width="297dp"
        android:layout_height="62dp"
        android:layout_marginBottom="544dp"
        android:ems="10"
        android:hint="輸入姓名"
        android:inputType="textPersonName"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/button1"
        android:layout_width="142dp"
        android:layout_height="65dp"
        android:layout_marginBottom="104dp"
        android:text="確認"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>